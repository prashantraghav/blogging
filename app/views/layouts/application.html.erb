<!DOCTYPE html>
<html lang="en">
  <head>
    <%= render 'layouts/partials/application/head' %>
  </head>
  <body>
    <div id="wrapper">
      <%= render 'layouts/partials/application/nav' %>
      <div id="page-wrapper">
        <div class="container">
          <br/><br/>
          <div class="row">
            <div class="col-lg-12 col-sm-12 col-xs-12 col-md-12">
              <% if notice %> <div class="alert alert-success"><%= notice %></div><% end %>
              <% if alert %><div class="alert alert-danger"><%= alert %></div><% end %>
              <%= yield %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <%= render 'layouts/partials/application/footer' %>
  </body>
</html>
